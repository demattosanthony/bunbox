{
  "name": "@ademattos/bunbox-openapi",
  "version": "0.1.0",
  "description": "OpenAPI/Swagger documentation plugin for Bunbox",
  "author": "Anthony Demattos <demattosanthony@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/demattosanthony/bunbox.git",
    "directory": "packages/bunbox-openapi"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
